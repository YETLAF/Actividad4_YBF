import Foundation

//condicionales y ciclos
var datos = [3,6,9,2,4,1] //datos con los valores [3,6,9,2,4,1]

for dato in datos where dato < 5 { //la variable dato guarda los valores menores a 5
    print("Números menores a 5: \(dato)")
}
//Funciones
func Suma(val1;Int, val2;Int) Int{
    return val1 + val2
    }

//Potencia
public struct Potencia
{
    // Número que queremos elevar
    private var value: Int
    // Potencia a la que vamos a elevar el número
    private var power: Int

    public var resultado: Int
    {
        get
        {
            Int(pow(Double(self.value), Double(self.power)))
        }
        
        set
        {
            self.value = newValue
        }
    }

    // Potencia
    public init(_ power: Int)
    {
        self.power = power
        self.value = 0
    }

    // elevamos

    public init(resultado: Int, _ power: Int)
    {
        self.power = power
        self.value = resultado
    }
}

