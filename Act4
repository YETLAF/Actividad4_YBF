var n = 12
switch n {
case 1:
    print("Elegiste Enero")
case 2:
    print("Elegiste Febrero")
case 3:
    print("Elegiste Marzo")
case 4:
    print("Elegiste Abril")
case 5:
    print("Elegiste Mayo")
case 6:
    print("Elegiste Junio")
case 7:
    print("Elegiste Julio")
case 8:
    print("Elegiste Agosto")
case 9:
    print("Elegiste Septiembre")
case 10:
    print("Elegiste Octubre")
case 11:
    print("Elegiste Noviembre")
case 12:
    print("Elegiste Diciembre")
}

